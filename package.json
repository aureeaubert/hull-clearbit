{
  "name": "hull-clearbit",
  "description": "Enrich Customer data with Clearbit",
  "version": "0.3.3",
  "homepage": "https://github.com/hull-ships/hull-clearbit",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "8.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-clearbit"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-clearbit/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain clearbit",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "./node_modules/.bin/babel server -d build",
    "start": "node -r newrelic ./build",
    "start:dev": "LOG_LEVEL=debug NODE_ENV=development ./node_modules/.bin/nodemon -d 2 -V -w server -x ./node_modules/.bin/babel-node -- server",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "./node_modules/.bin/eslint server",
    "test:modules": "npm outdated --depth=0",
    "test:unit": "NODE_ENV=test mocha ./test/unit/*.js",
    "test:integration": "NODE_ENV=test mocha ./test/integration/*.js",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf build",
    "update": "./node_modules/.bin/updtr",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "minihull": "^2.1.0",
    "mocha": "^4.0.1",
    "nock": "^9.1.5",
    "nodemon": "^1.14.1",
    "sinon": "^4.1.3",
    "supertest": "^3.0.0",
    "updtr": "^2.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bottleneck": "1.16.0",
    "clearbit": "1.3.3",
    "connect": "^3.6.5",
    "ejs": "^2.5.7",
    "event-stream": "^3.3.4",
    "express": "^4.16.2",
    "find-up": "^2.1.0",
    "hull": "0.13.9",
    "jquery": "^3.2.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "newrelic": "^2.4.2",
    "object-mapper": "^4.2.0",
    "qs": "^6.5.1",
    "range_check": "^1.4.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "select2": "^4.0.6-rc.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.2"
  }
}
